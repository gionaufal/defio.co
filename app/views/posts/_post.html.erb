<div class="col m6 s12">
  <div class="card center-align">
    <div class="card-image">
      <%= image_tag post.image %>
    </div>
    <div class="card-content">
      <p><%= post.description %></p>
    </div>
    <div class="card-action">
      <%= link_to 'More',post %>
    </div>
  </div>
</div>
